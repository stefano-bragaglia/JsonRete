#!/usr/bin/env python
import logging

import coloredlogs
import verboselogs

verboselogs.install()
coloredlogs.install()

_logger = logging.getLogger('JsonRete')


def main():
    while True:
        command = input('>>> ')
        if command is 'exit':
            break
        print(command.upper())


if __name__ == '__main__':
    main()
    _logger.verbose('Done.')
